<% provide(:title, "About") %>
<!-- provide function and associate the string 
	"Home" with the label :title -->
<!DOCTYPE html>
<!-- a missing template, which in the context of 
	Rails is essentially the same thing as a view -->
<html>
  <head>
<title><%= yield(:title) %> | This is where it Starts </title>
<%= stylesheet_link_tag    'application', media: 'all',                                       'data-turbolinks-track' => true %>
 <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
 <%= csrf_meta_tags %> <!-- prevents malicious web attacks -->
</head>
  <body>
<h1> About </h1>
<p>
  This is the about page. Not much to see, but much to come. Give it time and all shall be well!
  <br><br><a href="home">Home</a>.<br>
  <br><a href="help">Help</a>.
  
</p>

</body>
</html>


